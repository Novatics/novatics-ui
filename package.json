{
  "name": "novatics",
  "version": "0.0.0",
  "license": "MIT",
  "private": false,
  "publishConfig": {
    "@novatics:registry": "https://npm.pkg.github.com"
  },
  "scripts": {
    "nx": "nx",
    "dev": "nx storybook core",
    "build": "nx run-many --all --target=build",
    "generate:component": "nx g lib --buildable --publishable --compiler=swc --style=@emotion/styled",
    "generate:storybook": "nx g storybook-configuration --configureCypress=false --generateCypressSpecs=false --generateStories=true",
    "component:generate": "nx workspace-generator component",
    "prepare": "husky install",
    "lint": "eslint ./packages/**/src/**/*.tsx ./packages/**/src/*.ts --fix",
    "verdaccio:up": "cp .npmrc .npmrc.bkp && cp .npmrc.local .npmrc && docker-compose up -d --build",
    "verdaccio:addUser": "npm adduser --registry http://localhost:4873/",
    "verdaccio:down": "docker-compose stop && cp .npmrc.bkp .npmrc",
    "versions:create": "nx affected --target=version --base=last-release --noVerify --verbose --parallel=1 --trackDeps --skipCommitTypes=docs,ci,build",
    "versions:create:push": "nx affected --target=version --base=last-release --noVerify --verbose --parallel=1 --trackDeps --skipCommitTypes=docs,ci,build --push",
    "versions:create:deploy": "nx affected --target=version:deploy --base=last-release --noVerify --verbose --parallel=1 --trackDeps --skipCommitTypes=docs,ci,build",
    "versions:create:push:deploy": "nx affected --target=version:deploy --base=last-release --noVerify --verbose --parallel=1 --trackDeps --skipCommitTypes=docs,ci,build --push",
    "versions:deploy": "nx affected --base=last-release --verbose --target deploy",
    "storybook:build": "nx build-storybook core"
  },
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@mui/icons-material": "^5.10.16",
    "@mui/material": "^5.10.17",
    "@storybook/addon-docs": "^6.5.14",
    "core-js": "^3.26.1",
    "currency.js": "^2.0.4",
    "lodash.defaults": "^4.2.0",
    "lodash.pick": "^4.4.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-simple-wheel-picker": "^1.2.0",
    "regenerator-runtime": "0.13.11",
    "tslib": "^2.4.1"
  },
  "devDependencies": {
    "@babel/core": "7.20.5",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "7.18.6",
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@commitlint/config-nx-scopes": "^17.2.0",
    "@emotion/babel-plugin": "11.10.5",
    "@jscutlery/semver": "^2.29.1",
    "@nrwl/cli": "15.3.0",
    "@nrwl/devkit": "15.3.0",
    "@nrwl/eslint-plugin-nx": "15.3.0",
    "@nrwl/jest": "15.3.0",
    "@nrwl/js": "15.3.0",
    "@nrwl/linter": "15.3.0",
    "@nrwl/react": "^15.3.0",
    "@nrwl/storybook": "^15.3.0",
    "@nrwl/web": "15.3.0",
    "@nrwl/workspace": "15.3.0",
    "@react-theming/storybook-addon": "^1.1.9",
    "@storybook/addon-essentials": "~6.5.14",
    "@storybook/builder-webpack5": "~6.5.14",
    "@storybook/core-server": "~6.5.14",
    "@storybook/manager-webpack5": "~6.5.14",
    "@storybook/react": "~6.5.14",
    "@svgr/webpack": "^6.5.1",
    "@swc/core": "~1.3.21",
    "@swc/helpers": "~0.4.11",
    "@swc/jest": "0.2.23",
    "@testing-library/react": "13.4.0",
    "@types/jest": "29.2.4",
    "@types/lodash": "^4.14.191",
    "@types/lodash.defaults": "^4.2.7",
    "@types/lodash.includes": "^4.3.7",
    "@types/lodash.keys": "^4.2.7",
    "@types/lodash.pick": "^4.4.7",
    "@types/lodash.snakecase": "^4.1.7",
    "@types/lodash.sortby": "^4.7.7",
    "@types/lodash.startcase": "^4.4.7",
    "@types/node": "18.11.11",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.9",
    "@typescript-eslint/eslint-plugin": "~5.46.0",
    "@typescript-eslint/parser": "~5.46.0",
    "babel-jest": "29.3.1",
    "babel-loader": "9.1.0",
    "commitizen": "^4.2.6",
    "commitlint": "^17.3.0",
    "cz-customizable": "^7.0.0",
    "eslint": "~8.29.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-react": "7.31.11",
    "eslint-plugin-react-hooks": "4.6.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.2",
    "jest": "29.3.1",
    "jest-environment-jsdom": "29.3.1",
    "lerna": "^6.1.0",
    "lodash.includes": "^4.3.0",
    "lodash.keys": "^4.2.0",
    "lodash.snakecase": "^4.1.1",
    "lodash.sortby": "^4.7.0",
    "lodash.startcase": "^4.4.0",
    "ngx-deploy-npm": "^4.3.10",
    "nx": "15.3.0",
    "prettier": "^2.8.1",
    "react-test-renderer": "18.2.0",
    "storybook-addon-swc": "^1.1.9",
    "swc-loader": "0.2.3",
    "ts-jest": "29.0.3",
    "ts-node": "10.9.1",
    "typescript": "^4.9.4",
    "url-loader": "^4.1.1"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  }
}
