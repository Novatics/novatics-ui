    "@nx/storybook": "16.5.5",
    "@nx/vite": "16.5.5",
    "@nx/web": "16.5.5",
    "@nx/webpack": "16.5.5",
    "@nx/workspace": "16.5.5",
    "@react-theming/storybook-addon": "^1.1.10",
    "@rollup/plugin-url": "^8.0.1",
    "@storybook/addon-docs": "^7.1.0",
    "@storybook/addon-essentials": "7.1.0",
    "@storybook/addon-styling": "^1.3.4",
