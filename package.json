{
  "name": "novatics-ui",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "nx storybook core",
    "build": "nx run-many --all --target=build",
    "generate:component": "nx g lib --buildable --publishable --compiler=swc --style=@emotion/styled",
    "generate:storybook": "nx g storybook-configuration --configureCypress=false --generateCypressSpecs=false --generateStories=true",
    "component:generate": "nx workspace-generator component",
    "prepare": "husky install",
    "lint": "eslint ./packages/**/src/**/*.tsx ./packages/**/src/*.ts --fix",
    "verdaccio:up": "cp .npmrc .npmrc.bkp && cp .npmrc.local .npmrc && docker-compose up -d --build",
    "verdaccio:addUser": "npm adduser --registry http://localhost:4873/",
    "verdaccio:down": "docker-compose stop && cp .npmrc.bkp .npmrc",
    "versions:create": "nx affected --target version --noVerify --verbose --parallel=1 --trackDeps --skipCommitTypes=docs,ci,build",
    "versions:create:push": "nx affected --target version --noVerify --verbose --parallel=1 --trackDeps --skipCommitTypes=docs,ci,build --push",
    "versions:create:deploy": "nx affected --target version:deploy --noVerify --verbose --parallel=1 --trackDeps --skipCommitTypes=docs,ci,build",
    "versions:create:push:deploy": "nx affected --target version:deploy --noVerify --verbose --parallel=1 --trackDeps --skipCommitTypes=docs,ci,build --push",
    "versions:deploy": "nx affected --target deploy"
  },
  "private": true,
  "dependencies": {
    "@emotion/react": "11.10.4",
    "@emotion/styled": "11.10.4",
    "@mui/material": "^5.10.9",
    "@mui/x-date-pickers": "^5.0.5",
    "@storybook/addon-docs": "^6.5.10",
    "core-js": "^3.6.5",
    "currency.js": "^2.0.4",
    "date-fns": "^2.29.3",
    "lodash.defaults": "^4.2.0",
    "lodash.pick": "^4.4.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "regenerator-runtime": "0.13.7",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "7.12.13",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "7.12.13",
    "@commitlint/cli": "^17.0.0",
    "@commitlint/config-conventional": "^17.0.0",
    "@commitlint/config-nx-scopes": "^17.0.0",
    "@emotion/babel-plugin": "11.10.2",
    "@jscutlery/semver": "^2.27.1",
    "@nrwl/cli": "15.0.0",
    "@nrwl/devkit": "15.0.0",
    "@nrwl/eslint-plugin-nx": "15.0.0",
    "@nrwl/jest": "15.0.0",
    "@nrwl/js": "15.0.0",
    "@nrwl/linter": "15.0.0",
    "@nrwl/react": "15.0.0",
    "@nrwl/storybook": "15.0.0",
    "@nrwl/web": "15.0.0",
    "@nrwl/workspace": "15.0.0",
    "@react-theming/storybook-addon": "^1.1.7",
    "@storybook/addon-essentials": "~6.5.9",
    "@storybook/builder-webpack5": "~6.5.9",
    "@storybook/core-server": "~6.5.9",
    "@storybook/manager-webpack5": "~6.5.9",
    "@storybook/react": "~6.5.9",
    "@svgr/webpack": "^5.4.0",
    "@swc/core": "~1.3.9",
    "@swc/jest": "0.2.23",
    "@testing-library/react": "13.4.0",
    "@types/jest": "28.1.1",
    "@types/lodash": "^4.14.186",
    "@types/lodash.defaults": "^4.2.7",
    "@types/lodash.includes": "^4.3.7",
    "@types/lodash.keys": "^4.2.7",
    "@types/lodash.pick": "^4.4.7",
    "@types/lodash.snakecase": "^4.1.7",
    "@types/lodash.sortby": "^4.7.7",
    "@types/lodash.startcase": "^4.4.7",
    "@types/node": "16.11.7",
    "@types/react": "18.0.20",
    "@types/react-dom": "18.0.6",
    "@typescript-eslint/eslint-plugin": "~5.40.1",
    "@typescript-eslint/parser": "~5.40.1",
    "babel-jest": "28.1.1",
    "babel-loader": "8.1.0",
    "commitizen": "^4.2.5",
    "commitlint": "^17.1.2",
    "cz-customizable": "^7.0.0",
    "eslint": "~8.25.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-react": "7.31.8",
    "eslint-plugin-react-hooks": "4.6.0",
    "husky": "^8.0.0",
    "jest": "28.1.1",
    "jest-environment-jsdom": "28.1.1",
    "lerna": "^6.0.1",
    "lodash.includes": "^4.3.0",
    "lodash.keys": "^4.2.0",
    "lodash.snakecase": "^4.1.1",
    "lodash.sortby": "^4.7.0",
    "lodash.startcase": "^4.4.0",
    "ngx-deploy-npm": "^4.2.5",
    "nx": "15.0.0",
    "nxpm": "^2.0.0",
    "prettier": "^2.7.1",
    "react-test-renderer": "18.2.0",
    "storybook-addon-swc": "^1.1.9",
    "swc-loader": "0.2.3",
    "ts-jest": "28.0.5",
    "ts-node": "10.9.1",
    "typescript": "~4.8.4",
    "url-loader": "^3.0.0"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  }
}
